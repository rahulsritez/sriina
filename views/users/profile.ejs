    <body class="cnt-home">
        <header class="header-style-1">
            <%- include('../front/top_bar') -%> <%- include('../front/search') -%> <%- include('../front/navigation') -%>
        </header>

		<div class="breadcrumb">
			<div class="container">
				<div class="breadcrumb-inner">
					<ul class="list-inline list-unstyled">
						<li><a href="/">Home</a></li>
						<li class="active">My Profile</li>
					</ul>
				</div>
				<!-- /.breadcrumb-inner -->
			</div>
			<!-- /.container -->
		</div>
		<!-- /.breadcrumb -->

		<div class="body-content">
			<div class="container">
				<div class="checkout-box faq-page">
					<div class="col-md-6 col-sm-6 create-new-account">
                            <h4 class="checkout-subtitle">Personal Information (<small>Sriina Customer</small>)</h4>

                            <form action="#" method="post" class="register-form outer-top-xs" enctype="application/x-www-form-urlencoded" id="register-form" onsubmit="return validateRegister()" autocomplete="off">
                                <div class="form-group success">
                                    <label class="info-title" for="exampleInputEmail2">Email Address <span>*</span></label>
                                    <input readonly="readonly" type="email" class="form-control success unicase-form-control text-input" value="<%= data.email %>">
                                </div>
                                <div class="form-group error">
                                    <label class="info-title" for="exampleInputEmail1">Name <span>*</span></label>
                                    <input readonly="readonly" type="text" class="form-control error unicase-form-control text-input" value="<%= data.name %>">

                                </div>
                                <div class="form-group">
                                    <label class="info-title" for="exampleInputEmail1">Phone Number <span>*</span></label>
                                    <input readonly="readonly" type="text" class="form-control unicase-form-control text-input" value="<%= data.mobile %>">

                                </div>
                                <!--<div class="form-group">
                                    <label class="info-title" for="exampleInputEmail1">Password <span>*</span></label>
                                    <input type="password" name="password" placeholder="Please enter password" class="form-control unicase-form-control text-input" id="password">
                                    <i class="fa fa-check-circle"></i>
                                    <i class="fa fa-exclamation-circle"></i>
                                    <small id="error_pwd"></small>
                                </div>-->
                                <!-- <input type="hidden" name="login" value="true"> -->
                                <button type="button" class="btn-upper btn btn-primary checkout-page-button">Submit</button>
                            </form>
                        </div>
				</div>
			</div>
		</div>

<%- include('../front/book_footer') -%>
