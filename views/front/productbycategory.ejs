<script src="<%= baseURL %>assets/js/bootstrap-select.min.js"></script> 
<script src="<%= baseURL %>assets/js/wow.min.js"></script> 
<script src="<%= baseURL %>assets/js/scripts.js"></script>
<script src="<%= baseURL %>assets/js/readmore.js"></script>
</body>
</html>
