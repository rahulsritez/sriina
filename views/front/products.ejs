        <header class="header-style-1">
            <%- include('../front/top_bar') -%> <%- include('../front/search') -%> <%- include('../front/navigation') -%>
        </header>

        <div class="breadcrumb">
            <div class="container">
                <div class="breadcrumb-inner">
                    <ul class="list-inline list-unstyled">
                        <li><a href="/">Home</a></li>
                        <li class="active">All Books</li>
                    </ul>
                </div>
                <!-- /.breadcrumb-inner -->
            </div>
            <!-- /.container -->
        </div>
        <!-- /.breadcrumb -->

        <div class="body-content">
            <div class="container">
                <div class="sign-in-page">
                    <div class="row">
                        <!-- Sign-in -->
                        <div class="col-md-12 col-sm-12 sign-in">
                            <% if (myJsonString.products.length > 0) {%>
                                <% myJsonString.products.forEach((list, index) => { %>
                                <% var new_price = (list.price)-(list.price/100)*list.discount; %>
                                    <div class="col-md-3">
                                      <div class="products">
                                          <div class="product">
                                              <div class="product-image_single">
                                                  <% if(list.image) {%>
                                                      <div class="image"> 
                                                        <% if(/(http(s?)):\/\//i.test(list.image)) {%>
                                                          <a href="<%= list.slug %>/<%= list.id %>"><img src="<%= list.image %>" alt=""></a> 
                                                        <% } else { %>
                                                          <a href="<%= list.slug %>/<%= list.id %>"><img src="<%= process.env.IMAGE_URL %><%= list.image %>" alt=""></a> 
                                                        <% } %>
                                                      </div>
                                                  <% } else { %>
                                                      <div class="image"> 
                                                          <a href="<%= list.slug %>/<%= list.id %>"><img src="<%= process.env.IMAGE_URL %>404.jpg" alt=""></a> 
                                                      </div>
                                                  <% } %>
                                              </div>
                                              <div class="product-info text-left">
                                                  <div style="min-height:110px;">
                                                  <h3 class="name"><a href="<%= list.slug %>/<%= list.id %>"><%= list.name.substring(0, 60) %></a></h3>
                                                  <div class="rating rateit-small"></div>
                                                  <div class="description"></div>
                                                  <div class="product-price">
                                                      <span class="price"> ₹<%= new_price.toFixed(2).replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,") %> </span> <span class="price-before-discount">₹ <%= (list.price) %></span> <span><%= list.discount %>% off</span> </div></div>
                                                  <!-- <% if( maxplan[0].MaxPlanPrice >= list.price ) { %>
                                                      <div class="on_rent"><a href="membership?productId=<%= list.id %>">On Rent</a></div>
                                                  <% } %>
                           -->
                                                   <div class="add_cart">
                                                  <a class="add-cart" href="<%= list.slug %>/<%= list.id %>"><i class="fa fa-shopping-cart"></i> View Product </a>
                                                   </div>
                                                   <div class="clearfix"></div>
                                              </div> 
                                          </div>
                                      </div>
                                    </div>
                                  <% }) %>
                              <% } else { %>
                                  <p class="text-center">No Book Found.</p>
                              <% } %>
                        </div>
                        <div class="text-center">
                            <% if (myJsonString.pages > 0) { %>
                                <ul class="pagination">
                                  <% if (myJsonString.current == 1) { %>
                                    <li class="page-item disabled"><a class="page-link">First</a></li>
                                <% } else { %>
                                    <li><a class="page-link" href="/products?page=1">First</a></li>
                                <% } %>
                                <% var i = (Number(myJsonString.current) > 5 ? Number(myJsonString.current) - 4 : 1) %>
                                <% if (i !== 1) { %>
                                    <li class="page-item disabled"><a>...</a></li>
                                <% } %>
                                <% for (; i <= (Number(myJsonString.current) + 4) && i <= myJsonString.pages; i++) { %>
                                    <% if (i == myJsonString.current) { %>
                                        <li class="page-item active"><a class="page-link"><%= i %></a></li>
                                    <% } else { %>
                                        <li><a class="page-link" href="/products?page=<%= i %>"><%= i %></a></li>
                                    <% } %>
                                    <% if (i == Number(myJsonString.current) + 4 && i < myJsonString.pages) { %>
                                        <li class="page-item disabled"><a class="page-link">...</a></li>
                                    <% } %>
                                <% } %>
                                <% if (myJsonString.current == myJsonString.pages) { %>
                                    <li class="page-item disabled"><a class="page-link">Last</a></li>
                                <% } else { %>
                                    <li><a class="page-link" href="/products?page=<%= myJsonString.pages %>">Last</a></li>
                                <% } %>
                                </ul>
                                <% } %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <%- include('../front/book_footer') -%>
